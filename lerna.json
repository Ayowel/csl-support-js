{
    "version": "independent",
    "command": {
      "publish": {
        "bump": "from-package",
        "throttle": true,
        "throttleDelay": 7200,
        "throttleSize": 40
      }
    },
    "packages": [
        "packages/csl-locale-all",
        "packages/csl-style-all",
        "packages/locales/*",
        "packages/styles/*"
    ]
}
